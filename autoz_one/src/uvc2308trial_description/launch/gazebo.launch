<launch>
  <param command="$(find xacro)/xacro $(find uvc2308trial_description)/urdf/uvc2308trial.xacro" name="robot_description"/>
  <node args="-param robot_description -urdf -model uvc2308trial" name="spawn_urdf" pkg="gazebo_ros" type="spawn_model"/>
  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <arg name="paused" value="false"/>
    <arg name="use_sim_time" value="true"/>
    <arg name="gui" value="true"/>
    <arg name="headless" value="false"/>
    <arg name="debug" value="false"/>
  </include>

  <node name="spawn_model_keytelop" pkg="teleop_twist_keyboard" type="teleop_twist_keyboard.py" output="screen" />
  <node name="posecorrector" pkg="uvc2308trial_description" type="posecorrector.py" output="screen" />
</launch>
